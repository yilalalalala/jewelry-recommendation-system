      "source": [
        "#Jewelry Data Import Script for Supabase\n",
        "\n",
        "!pip install pandas sqlalchemy psycopg2-binary kmodes faker -q\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sqlalchemy import create_engine, text\n",
        "from collections import Counter\n",
        "from kmodes.kmodes import KModes\n",
        "import json\n",
        "\n",
        "DATABASE_URL = \"postgresql://postgres.onvusagrrakeyboxuila:CaoNgodbs2025@aws-0-us-west-2.pooler.supabase.com:6543/postgres\"\n",
        "CSV_FILE = \"/content/ready_for_ml_adjusted_if.csv\"\n",
        "N_CLUSTERS = 8\n",
        "\n",
        "# COLUMN DEFINITIONS\n",
        "\n",
        "CATEGORY_COLS = ['bracelets', 'earrings', 'necklaces', 'rings']\n",
        "\n",
        "GEM_COLS = [\n",
        "    'amazonite', 'amethyst', 'amethysts', 'aquamarines', 'aventurine',\n",
        "    'carnelian', 'carnelians', 'chrysoprase', 'chrysoprases', 'citrine',\n",
        "    'coral', 'diamond', 'diamonds', 'emeralds', 'garnets', 'lapis lazuli',\n",
        "    'malachite', 'obsidians', 'onyx', 'pearl', 'mother_of_pearl',\n",
        "    'gray mother_of_pearl', 'white mother_of_pearl', 'peridots', 'pink sapphire',\n",
        "    'pink sapphires', 'rubies', 'sapphire', 'sapphires', 'spessartite garnet',\n",
        "    'spinels', 'tsavorite garnet', 'tsavorite garnets', 'brown diamonds'\n",
        "]\n",
        "\n",
        "BAND_COLS = [\n",
        "    'pink gold', 'white gold', 'yellow gold', 'platinum',\n",
        "    'non_rhodiumized white gold', 'ceramic', 'black ceramic',\n",
        "    'lacquer', 'black lacquer'\n",
        "]\n",
        "\n",
        "GEM_COLORS = {\n",
        "    'amazonite': 'green', 'amethyst': 'purple', 'amethysts': 'purple',\n",
        "    'aquamarines': 'blue', 'aventurine': 'green',\n",
        "    'carnelian': 'orange', 'carnelians': 'orange',\n",
        "    'chrysoprase': 'green', 'chrysoprases': 'green',\n",
        "    'citrine': 'yellow', 'coral': 'red',\n",
        "    'diamond': 'white', 'diamonds': 'white', 'brown diamonds': 'brown',\n",
        "    'emeralds': 'green', 'garnets': 'red', 'lapis lazuli': 'blue',\n",
        "    'malachite': 'green', 'obsidians': 'black', 'onyx': 'black',\n",
        "    'pearl': 'white', 'mother_of_pearl': 'gray', 'gray mother_of_pearl': 'gray',\n",
        "    'white mother_of_pearl': 'iridescent white', 'peridots': 'green',\n",
        "    'pink sapphire': 'pink', 'pink sapphires': 'pink',\n",
        "    'rubies': 'red', 'sapphire': 'blue', 'sapphires': 'blue',\n",
        "    'spessartite garnet': 'orange', 'spinels': 'red',\n",
        "    'tsavorite garnet': 'green', 'tsavorite garnets': 'green'\n",
        "}\n",
        "\n",
        "BAND_COLORS = {\n",
        "    'pink gold': 'pink', 'white gold': 'silver', 'yellow gold': 'gold',\n",
        "    'platinum': 'silver', 'non_rhodiumized white gold': 'silver',\n",
        "    'ceramic': 'white', 'black ceramic': 'black',\n",
        "    'lacquer': 'red', 'black lacquer': 'black'\n",
        "}\n",
        "\n",
        "# Style profiles\n",
        "STYLE_PROFILES = {\n",
        "    'classic': {\n",
        "        'gems': ['diamond', 'pearl', 'sapphire', 'sapphires'],\n",
        "        'bands': ['white gold', 'platinum']\n",
        "    },\n",
        "    'modern': {\n",
        "        'gems': ['brown diamonds', 'obsidians', 'aquamarines', 'tsavorite garnet', 'onyx'],\n",
        "        'bands': ['white gold', 'platinum', 'ceramic', 'black ceramic']\n",
        "    },\n",
        "    'romantic': {\n",
        "        'gems': ['pink sapphire', 'pink sapphires', 'rubies', 'coral', 'pearl', 'spinels'],\n",
        "        'bands': ['pink gold']\n",
        "    },\n",
        "    'bold': {\n",
        "        'gems': ['emeralds', 'rubies', 'sapphire', 'sapphires', 'garnets', 'peridots', 'spessartite garnet'],\n",
        "        'bands': ['yellow gold', 'black lacquer']\n",
        "    },\n",
        "    'bohemian': {\n",
        "        'gems': ['amazonite', 'aventurine', 'carnelian', 'carnelians', 'malachite',\n",
        "                 'chrysoprase', 'lapis lazuli', 'coral'],\n",
        "        'bands': ['yellow gold', 'pink gold']\n",
        "    },\n",
        "    'minimalist': {\n",
        "        'gems': ['aquamarines', 'carnelian', 'amethyst'],\n",
        "        'bands': ['white gold', 'platinum']\n",
        "    },\n",
        "    'vintage': {\n",
        "        'gems': ['pearl', 'amethyst', 'amethysts', 'citrine', 'peridots', 'mother_of_pearl'],\n",
        "        'bands': ['yellow gold', 'pink gold', 'black lacquer']\n",
        "    },\n",
        "    'luxurious': {\n",
        "        'gems': ['diamond', 'diamonds'],\n",
        "        'bands': ['platinum', 'white gold']\n",
        "    }\n",
        "}\n",
        "# HELPER FUNCTIONS\n",
        "\n",
        "def extract_collection(title):\n",
        "    \"\"\"Extract collection name from product title\"\"\"\n",
        "    collections = [\n",
        "        'Love', 'Panthère', 'Trinity', 'Juste un Clou', 'Amulette',\n",
        "        'Cactus', 'Clash', 'Ecrou', 'Diamants Légers', 'Etincelle',\n",
        "        'Reflection', 'Agrafe', 'Pluie', 'Essential Lines', 'Santos',\n",
        "        'C de Cartier', 'Cartier Destinée', 'Maillon', 'Hearts and Symbols'\n",
        "    ]\n",
        "    for collection in collections:\n",
        "        if collection.lower() in title.lower():\n",
        "            return collection\n",
        "    return 'Other'\n",
        "\n",
        "\n",
        "def get_price_tier(price):\n",
        "    \"\"\"Categorize price into tiers\"\"\"\n",
        "    if price < 3000:\n",
        "        return 'budget'\n",
        "    elif price < 10000:\n",
        "        return 'mid'\n",
        "    elif price < 50000:\n",
        "        return 'premium'\n",
        "    else:\n",
        "        return 'luxury'\n",
        "\n",
        "\n",
        "def get_category(row):\n",
        "    \"\"\"Determine category from binary columns\"\"\"\n",
        "    for cat in CATEGORY_COLS:\n",
        "        if row.get(cat, 0) == 1:\n",
        "            return cat\n",
        "    return 'rings'\n",
        "\n",
        "\n",
        "def get_item_colors(row):\n",
        "    \"\"\"Get colors for an item based on materials\"\"\"\n",
        "    colors = set()\n",
        "    for gem, color in GEM_COLORS.items():\n",
        "        if gem in row.index and row.get(gem, 0) == 1:\n",
        "            colors.add(color)\n",
        "    for band, color in BAND_COLORS.items():\n",
        "        if band in row.index and row.get(band, 0) == 1:\n",
        "            colors.add(color)\n",
        "    return list(colors) if colors else ['neutral']\n",
        "\n",
        "\n",
        "def get_clustering_features(df):\n",
        "    \"\"\"Get features for K-Modes clustering\"\"\"\n",
        "    feature_cols = []\n",
        "    for col in CATEGORY_COLS + GEM_COLS + BAND_COLS:\n",
        "        if col in df.columns:\n",
        "            feature_cols.append(col)\n",
        "    return df[feature_cols].values, feature_cols\n",
        "\n",
        "\n",
        "def calculate_material_weights(df):\n",
        "    \"\"\"Calculate inverse document frequency weights for materials\"\"\"\n",
        "    total_items = len(df)\n",
        "    weights = {}\n",
        "\n",
        "    for col in GEM_COLS + BAND_COLS:\n",
        "        if col in df.columns:\n",
        "            count = df[col].sum()\n",
        "            if count > 0:\n",
        "                weights[col] = np.log(total_items / (count + 1))\n",
        "\n",
        "    return weights\n",
        "\n",
        "\n",
        "def assign_styles_to_clusters(df, cluster_labels, n_clusters, material_weights):\n",
        "    \"\"\"Assign styles to clusters based on material composition\"\"\"\n",
        "    cluster_styles = {}\n",
        "\n",
        "    for cluster_id in range(n_clusters):\n",
        "        cluster_data = df[cluster_labels == cluster_id]\n",
        "        cluster_size = len(cluster_data)\n",
        "\n",
        "        if cluster_size == 0:\n",
        "            cluster_styles[cluster_id] = {\n",
        "                'primary_style': 'neutral',\n",
        "                'primary_score': 0,\n",
        "                'all_styles': {},\n",
        "                'top_3_styles': []\n",
        "            }\n",
        "            continue\n",
        "\n",
        "        style_scores = {}\n",
        "\n",
        "        for style_name, style_def in STYLE_PROFILES.items():\n",
        "            # Weighted gem score\n",
        "            gem_score = 0\n",
        "            gem_weight_sum = 0\n",
        "            for gem in style_def['gems']:\n",
        "                if gem in cluster_data.columns:\n",
        "                    frequency = cluster_data[gem].sum() / cluster_size\n",
        "                    rarity_weight = material_weights.get(gem, 1.0)\n",
        "                    gem_score += frequency * rarity_weight\n",
        "                    gem_weight_sum += rarity_weight\n",
        "\n",
        "            gem_score = gem_score / gem_weight_sum if gem_weight_sum > 0 else 0\n",
        "\n",
        "            # Weighted band score\n",
        "            band_score = 0\n",
        "            band_weight_sum = 0\n",
        "            for band in style_def['bands']:\n",
        "                if band in cluster_data.columns:\n",
        "                    frequency = cluster_data[band].sum() / cluster_size\n",
        "                    rarity_weight = material_weights.get(band, 1.0)\n",
        "                    band_score += frequency * rarity_weight\n",
        "                    band_weight_sum += rarity_weight\n",
        "\n",
        "            band_score = band_score / band_weight_sum if band_weight_sum > 0 else 0\n",
        "\n",
        "            # Combined score (60% gem, 40% band)\n",
        "            total_score = gem_score * 0.60 + band_score * 0.40\n",
        "            style_scores[style_name] = round(total_score, 4)\n",
        "\n",
        "        sorted_styles = sorted(style_scores.items(), key=lambda x: x[1], reverse=True)\n",
        "\n",
        "        cluster_styles[cluster_id] = {\n",
        "            'primary_style': sorted_styles[0][0] if sorted_styles[0][1] > 0 else 'neutral',\n",
        "            'primary_score': sorted_styles[0][1],\n",
        "            'all_styles': {s: score for s, score in sorted_styles if score > 0},\n",
        "            'top_3_styles': sorted_styles[:3]\n",
        "        }\n",
        "\n",
        "    return cluster_styles\n",
        "\n",
        "\n",
        "def generate_cluster_profiles(df, cluster_labels, n_clusters):\n",
        "    \"\"\"Generate descriptive profiles for each cluster\"\"\"\n",
        "    profiles = {}\n",
        "\n",
        "    for cluster_id in range(n_clusters):\n",
        "        cluster_data = df[cluster_labels == cluster_id]\n",
        "\n",
        "        profile = {\n",
        "            'size': len(cluster_data),\n",
        "            'avg_price': cluster_data['price'].mean() if len(cluster_data) > 0 else 0,\n",
        "            'categories': [],\n",
        "            'top_gems': [],\n",
        "            'top_bands': [],\n",
        "            'dominant_colors': []\n",
        "        }\n",
        "\n",
        "        if len(cluster_data) == 0:\n",
        "            profiles[cluster_id] = profile\n",
        "            continue\n",
        "\n",
        "        # Categories\n",
        "        for cat in CATEGORY_COLS:\n",
        "            if cat in cluster_data.columns:\n",
        "                count = cluster_data[cat].sum()\n",
        "                if count > 0:\n",
        "                    profile['categories'].append((cat, int(count)))\n",
        "        profile['categories'].sort(key=lambda x: x[1], reverse=True)\n",
        "\n",
        "        # Top gems\n",
        "        for gem in GEM_COLS:\n",
        "            if gem in cluster_data.columns:\n",
        "                count = cluster_data[gem].sum()\n",
        "                if count > 0:\n",
        "                    profile['top_gems'].append((gem, int(count)))\n",
        "        profile['top_gems'].sort(key=lambda x: x[1], reverse=True)\n",
        "\n",
        "        # Top bands\n",
        "        for band in BAND_COLS:\n",
        "            if band in cluster_data.columns:\n",
        "                count = cluster_data[band].sum()\n",
        "                if count > 0:\n",
        "                    profile['top_bands'].append((band, int(count)))\n",
        "        profile['top_bands'].sort(key=lambda x: x[1], reverse=True)\n",
        "\n",
        "        # Dominant colors\n",
        "        color_counts = Counter()\n",
        "        for _, row in cluster_data.iterrows():\n",
        "            colors = get_item_colors(row)\n",
        "            color_counts.update(colors)\n",
        "        profile['dominant_colors'] = color_counts.most_common(5)\n",
        "\n",
        "        profiles[cluster_id] = profile\n",
        "\n",
        "    return profiles\n",
        "# MAIN IMPORT FUNCTION\n",
        "\n",
        "def import_data():\n",
        "    print(\"=\" * 60)\n",
        "    print(\"JEWELRY DATA IMPORT TO SUPABASE\")\n",
        "    print(\"With K-Modes Clustering\")\n",
        "    print(\"=\" * 60)\n",
        "\n",
        "    # Connect to database\n",
        "    print(\"\\n[1/7] Connecting to Supabase\")\n",
        "    try:\n",
        "        engine = create_engine(DATABASE_URL)\n",
        "        with engine.connect() as conn:\n",
        "            conn.execute(text(\"SELECT 1\"))\n",
        "        print(\"Connected successfully\")\n",
        "    except Exception as e:\n",
        "        print(f\"Connection failed: {e}\")\n",
        "        return\n",
        "\n",
        "    # Read CSV\n",
        "    print(\"\\n[2/7] Reading CSV file\")\n",
        "    try:\n",
        "        df = pd.read_csv(CSV_FILE)\n",
        "        df.columns = df.columns.str.strip()\n",
        "        df = df.loc[:, ~df.columns.str.match(r'^Unnamed')]\n",
        "        df['ref'] = df['ref'].str.strip()\n",
        "        df['title'] = df['title'].str.strip()\n",
        "        print(f\"Loaded {len(df)} items from CSV\")\n",
        "    except Exception as e:\n",
        "        print(f\"Failed to read CSV: {e}\")\n",
        "        return\n",
        "\n",
        "    # Run K-Modes clustering\n",
        "    print(\"\\n[3/7] Running K-Modes clustering\")\n",
        "    features, feature_cols = get_clustering_features(df)\n",
        "\n",
        "    kmodes = KModes(\n",
        "        n_clusters=N_CLUSTERS,\n",
        "        init='Huang',\n",
        "        n_init=20,\n",
        "        verbose=0,\n",
        "        random_state=42\n",
        "    )\n",
        "    cluster_labels = kmodes.fit_predict(features)\n",
        "    df['cluster'] = cluster_labels\n",
        "    print(f\"Created {N_CLUSTERS} clusters\")\n",
        "\n",
        "    # Generate cluster profiles and assign styles\n",
        "    print(\"\\n[4/7] Assigning styles to clusters\")\n",
        "    material_weights = calculate_material_weights(df)\n",
        "    cluster_profiles = generate_cluster_profiles(df, cluster_labels, N_CLUSTERS)\n",
        "    cluster_styles = assign_styles_to_clusters(df, cluster_labels, N_CLUSTERS, material_weights)\n",
        "\n",
        "    for cid in range(N_CLUSTERS):\n",
        "        style = cluster_styles[cid]['primary_style']\n",
        "        score = cluster_styles[cid]['primary_score']\n",
        "        size = cluster_profiles[cid]['size']\n",
        "        print(f\"  Cluster {cid}: {style} (confidence: {score:.1%}, items: {size})\")\n",
        "\n",
        "    # Get lookup mappings\n",
        "    print(\"\\n[5/7] Loading lookup tables\")\n",
        "    with engine.connect() as conn:\n",
        "        categories = pd.read_sql(\"SELECT id, name FROM categories\", conn)\n",
        "        cat_map = dict(zip(categories['name'], categories['id']))\n",
        "\n",
        "        materials = pd.read_sql(\"SELECT id, name FROM materials\", conn)\n",
        "        mat_map = dict(zip(materials['name'], materials['id']))\n",
        "\n",
        "    print(f\"Loaded {len(cat_map)} categories, {len(mat_map)} materials\")\n",
        "\n",
        "    # Saving clusters to database\n",
        "    print(\"\\n[6/7] Saving clusters to database\")\n",
        "    with engine.connect() as conn:\n",
        "        for cid in range(N_CLUSTERS):\n",
        "            profile = cluster_profiles[cid]\n",
        "            style_info = cluster_styles[cid]\n",
        "\n",
        "            secondary = [s[0] for s in style_info['top_3_styles'][1:3]]\n",
        "            colors = [c[0] for c in profile['dominant_colors'][:5]]\n",
        "            gems = [g[0] for g in profile['top_gems'][:5]]\n",
        "            bands = [b[0] for b in profile['top_bands'][:3]]\n",
        "\n",
        "            conn.execute(text(\"\"\"\n",
        "                INSERT INTO clusters (id, primary_style, style_confidence, secondary_styles,\n",
        "                                      avg_price, item_count, dominant_colors, top_gems, top_bands)\n",
        "                VALUES (:id, :style, :confidence, :secondary, :price, :count, :colors, :gems, :bands)\n",
        "                ON CONFLICT (id) DO UPDATE SET\n",
        "                    primary_style = EXCLUDED.primary_style,\n",
        "                    style_confidence = EXCLUDED.style_confidence,\n",
        "                    secondary_styles = EXCLUDED.secondary_styles,\n",
        "                    avg_price = EXCLUDED.avg_price,\n",
        "                    item_count = EXCLUDED.item_count,\n",
        "                    dominant_colors = EXCLUDED.dominant_colors,\n",
        "                    top_gems = EXCLUDED.top_gems,\n",
        "                    top_bands = EXCLUDED.top_bands,\n",
        "                    updated_at = NOW()\n",
        "            \"\"\"), {\n",
        "                'id': int(cid),\n",
        "                'style': style_info['primary_style'],\n",
        "                'confidence': float(style_info['primary_score']),  # Convert to float\n",
        "                'secondary': secondary,\n",
        "                'price': float(profile['avg_price']),  # Convert to float\n",
        "                'count': int(profile['size']),  # Convert to int\n",
        "                'colors': colors,\n",
        "                'gems': gems,\n",
        "                'bands': bands\n",
        "            })\n",
        "        conn.commit()\n",
        "    print(\"Saved cluster profiles\")\n",
        "\n",
        "    # Import jewelry items\n",
        "    print(\"\\n[7/7] Importing jewelry items\")\n",
        "    success_count = 0\n",
        "    error_count = 0\n",
        "\n",
        "    with engine.connect() as conn:\n",
        "        for idx, row in df.iterrows():\n",
        "            try:\n",
        "                category = get_category(row)\n",
        "                collection = extract_collection(row['title'])\n",
        "                price_tier = get_price_tier(row['price'])\n",
        "                cluster_id = int(row['cluster'])\n",
        "\n",
        "                # Insert jewelry item\n",
        "                result = conn.execute(text(\"\"\"\n",
        "                    INSERT INTO jewelry_items\n",
        "                    (ref, title, price, image_url, quantity, size, chain_size,\n",
        "                     category_id, cluster_id, collection, price_tier)\n",
        "                    VALUES (:ref, :title, :price, :image, :quantity, :size, :chain_size,\n",
        "                            :cat_id, :cluster_id, :collection, :price_tier)\n",
        "                    ON CONFLICT (ref) DO UPDATE SET\n",
        "                        title = EXCLUDED.title,\n",
        "                        price = EXCLUDED.price,\n",
        "                        image_url = EXCLUDED.image_url,\n",
        "                        quantity = EXCLUDED.quantity,\n",
        "                        cluster_id = EXCLUDED.cluster_id,\n",
        "                        updated_at = NOW()\n",
        "                    RETURNING id\n",
        "                \"\"\"), {\n",
        "                    'ref': row['ref'],\n",
        "                    'title': row['title'],\n",
        "                    'price': int(row['price']),\n",
        "                    'image': row['image'],\n",
        "                    'quantity': int(row['quantity']),\n",
        "                    'size': str(row['size']) if pd.notna(row['size']) else None,\n",
        "                    'chain_size': str(row['chain_size']) if row['chain_size'] != 0 else None,\n",
        "                    'cat_id': cat_map.get(category),\n",
        "                    'cluster_id': cluster_id,\n",
        "                    'collection': collection,\n",
        "                    'price_tier': price_tier\n",
        "                })\n",
        "                item_id = result.fetchone()[0]\n",
        "\n",
        "                # Insert/update inventory\n",
        "                conn.execute(text(\"\"\"\n",
        "                    INSERT INTO inventory (item_id, stock_qty, status)\n",
        "                    VALUES (:item_id, :qty, :status)\n",
        "                    ON CONFLICT (item_id) DO UPDATE SET\n",
        "                        stock_qty = EXCLUDED.stock_qty,\n",
        "                        status = EXCLUDED.status,\n",
        "                        updated_at = NOW()\n",
        "                \"\"\"), {\n",
        "                    'item_id': item_id,\n",
        "                    'qty': int(row['quantity']),\n",
        "                    'status': 'active' if row['quantity'] > 0 else 'out_of_stock'\n",
        "                })\n",
        "\n",
        "                # Insert materials (gems and bands)\n",
        "                all_materials = GEM_COLS + BAND_COLS\n",
        "                for mat_col in all_materials:\n",
        "                    if mat_col in row.index and row.get(mat_col, 0) == 1:\n",
        "                        mat_id = mat_map.get(mat_col)\n",
        "                        if mat_id:\n",
        "                            conn.execute(text(\"\"\"\n",
        "                                INSERT INTO item_materials (item_id, material_id)\n",
        "                                VALUES (:item_id, :mat_id)\n",
        "                                ON CONFLICT (item_id, material_id) DO NOTHING\n",
        "                            \"\"\"), {'item_id': item_id, 'mat_id': mat_id})\n",
        "\n",
        "                success_count += 1\n",
        "\n",
        "                if (idx + 1) % 100 == 0:\n",
        "                    print(f\"  Processed {idx + 1}/{len(df)} items\")\n",
        "\n",
        "            except Exception as e:\n",
        "                error_count += 1\n",
        "                print(f\"Error on row {idx} (ref: {row.get('ref', 'unknown')}): {e}\")\n",
        "\n",
        "        conn.commit()\n",
        "\n",
        "    print(f\"\\nImport complete: {success_count} succeeded, {error_count} failed\")\n",
        "\n",
        "    # Print summary\n",
        "    print_summary(engine)\n",
        "\n",
        "    return df, cluster_profiles, cluster_styles\n",
        "\n",
        "\n",
        "def print_summary(engine):\n",
        "    \"\"\"Print import summary statistics\"\"\"\n",
        "    print(\"\\n\" + \"=\" * 50)\n",
        "    print(\"IMPORT SUMMARY\")\n",
        "    print(\"=\" * 50)\n",
        "\n",
        "    with engine.connect() as conn:\n",
        "        # Item count\n",
        "        result = conn.execute(text(\"SELECT COUNT(*) FROM jewelry_items\"))\n",
        "        print(f\"\\nTotal items: {result.fetchone()[0]}\")\n",
        "\n",
        "        # Cluster distribution\n",
        "        result = conn.execute(text(\"\"\"\n",
        "            SELECT cl.id, cl.primary_style, COUNT(j.id) as items\n",
        "            FROM clusters cl\n",
        "            LEFT JOIN jewelry_items j ON cl.id = j.cluster_id\n",
        "            GROUP BY cl.id, cl.primary_style\n",
        "            ORDER BY cl.id\n",
        "        \"\"\"))\n",
        "        print(\"\\nCluster Distribution:\")\n",
        "        for row in result.fetchall():\n",
        "            print(f\"  Cluster {row[0]} ({row[1]}): {row[2]} items\")\n",
        "\n",
        "        # Category distribution\n",
        "        result = conn.execute(text(\"\"\"\n",
        "            SELECT c.name, COUNT(j.id)\n",
        "            FROM categories c\n",
        "            LEFT JOIN jewelry_items j ON c.id = j.category_id\n",
        "            GROUP BY c.name\n",
        "            ORDER BY COUNT(j.id) DESC\n",
        "        \"\"\"))\n",
        "        print(\"\\nCategory Distribution:\")\n",
        "        for name, count in result.fetchall():\n",
        "            print(f\"  {name}: {count}\")\n",
        "\n",
        "        # Price tier distribution\n",
        "        result = conn.execute(text(\"\"\"\n",
        "            SELECT price_tier, COUNT(*)\n",
        "            FROM jewelry_items\n",
        "            GROUP BY price_tier\n",
        "            ORDER BY COUNT(*) DESC\n",
        "        \"\"\"))\n",
        "        print(\"\\nPrice Tier Distribution:\")\n",
        "        for tier, count in result.fetchall():\n",
        "            print(f\"  {tier}: {count}\")\n",
        "\n",
        "        # Materials linked\n",
        "        result = conn.execute(text(\"SELECT COUNT(DISTINCT item_id) FROM item_materials\"))\n",
        "        print(f\"\\nItems with materials linked: {result.fetchone()[0]}\")\n",
        "\n",
        "    print(\"=\" * 50)\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    import_data()"
      ]
    }
  ]
}