        "def convert_numpy_types(value):\n",
        "    import numpy as np\n",
        "    if isinstance(value, np.floating):\n",
        "        return float(value)\n",
        "    elif isinstance(value, np.integer):\n",
        "        return int(value)\n",
        "    elif isinstance(value, np.ndarray):\n",
        "        return value.tolist()\n",
        "    return value"
      ],
