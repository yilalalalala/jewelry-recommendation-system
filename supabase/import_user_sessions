        "        \"style_hint\": \"modern\",\n",
        "        \"colors\": [\"white\", \"black\", \"gray\", \"denim\"],\n",
        "        \"jewelry_types\": [\"rings\", \"earrings\"],\n",
        "        \"budget_multiplier\": 0.5,\n",
        "        \"weight\": 0.12\n",
        "    },\n",
        "    {\n",
        "        \"occasion\": \"vacation\",\n",
        "        \"style_hint\": \"bohemian\",\n",
        "        \"colors\": [\"turquoise\", \"orange\", \"coral\", \"white\"],\n",
        "        \"jewelry_types\": [\"necklaces\", \"bracelets\"],\n",
        "        \"budget_multiplier\": 0.8,\n",
        "        \"weight\": 0.10\n",
        "    }\n",
        "]\n",
        "\n",
        "# Color mapping for preference -> item matching\n",
        "COLOR_FAMILIES = {\n",
        "    'pink': ['pink', 'red', 'coral'],\n",
        "    'red': ['red', 'pink', 'coral'],\n",
        "    'white': ['white', 'silver', 'iridescent white'],\n",
        "    'silver': ['silver', 'white', 'gray'],\n",
        "    'gold': ['gold', 'yellow'],\n",
        "    'black': ['black', 'brown'],\n",
        "    'blue': ['blue'],\n",
        "    'green': ['green'],\n",
        "    'purple': ['purple'],\n",
        "    'orange': ['orange', 'yellow'],\n",
        "    'brown': ['brown', 'black']\n",
        "}\n",
        "\n",
        "# Sample names if Faker not available\n",
        "SAMPLE_FIRST_NAMES = [\n",
        "    \"Emma\", \"Olivia\", \"Ava\", \"Sophia\", \"Isabella\", \"Mia\", \"Charlotte\", \"Amelia\",\n",
        "    \"Harper\", \"Evelyn\", \"Luna\", \"Camila\", \"Sofia\", \"Scarlett\", \"Victoria\",\n",
        "    \"James\", \"William\", \"Benjamin\", \"Lucas\", \"Henry\", \"Alexander\", \"Daniel\"\n",
        "]\n",
        "SAMPLE_LAST_NAMES = [\n",
        "    \"Smith\", \"Johnson\", \"Williams\", \"Brown\", \"Jones\", \"Garcia\", \"Miller\",\n",
        "    \"Davis\", \"Rodriguez\", \"Martinez\", \"Chen\", \"Wang\", \"Kim\", \"Patel\"\n",
        "]\n",
        "\n",
        "# HELPER FUNCTIONS\n",
        "\n",
        "def weighted_choice(items):\n",
        "    \"\"\"Select item based on weight\"\"\"\n",
        "    weights = [item[\"weight\"] for item in items]\n",
        "    return random.choices(items, weights=weights, k=1)[0]\n",
        "\n",
        "\n",
        "def generate_name():\n",
        "    \"\"\"Generate a random name\"\"\"\n",
        "    if USE_FAKER:\n",
        "        return fake.name()\n",
        "    else:\n",
        "        return f\"{random.choice(SAMPLE_FIRST_NAMES)} {random.choice(SAMPLE_LAST_NAMES)}\"\n",
        "\n",
        "\n",
        "def generate_email(name):\n",
        "    \"\"\"Generate email from name\"\"\"\n",
        "    if USE_FAKER:\n",
        "        return fake.email()\n",
        "    else:\n",
        "        clean_name = name.lower().replace(\" \", \".\").replace(\"'\", \"\")\n",
        "        domains = [\"gmail.com\", \"yahoo.com\", \"outlook.com\", \"email.com\"]\n",
        "        return f\"{clean_name}{random.randint(1, 999)}@{random.choice(domains)}\"\n",
        "\n",
        "\n",
        "def generate_user_preferences(persona):\n",
        "    \"\"\"Generate user preferences matching ML model input format\"\"\"\n",
        "    budget_min, budget_max = persona[\"budget_range\"]\n",
        "\n",
        "    # Add randomness to budget\n",
        "    budget_min = int(budget_min * random.uniform(0.7, 1.3))\n",
        "    budget_max = int(budget_max * random.uniform(0.8, 1.4))\n",
        "\n",
        "    # Ensure valid range\n",
        "    if budget_max <= budget_min:\n",
        "        budget_max = budget_min + 2000\n",
        "\n",
        "    return {\n",
        "        \"favorite_styles\": persona[\"styles\"],\n",
        "        \"preferred_colors\": random.sample(persona[\"colors\"], k=min(2, len(persona[\"colors\"]))),\n",
        "        \"preferred_metals\": random.sample(persona[\"metals\"], k=min(2, len(persona[\"metals\"]))),\n",
        "        \"budget_min\": budget_min,\n",
        "        \"budget_max\": budget_max,\n",
        "        \"favorite_collections\": random.sample(persona[\"collections\"], k=min(2, len(persona[\"collections\"]))),\n",
        "        \"ring_size\": str(random.choice([4, 4.5, 5, 5.5, 6, 6.5, 7, 7.5, 8, 8.5, 9])),\n",
        "        \"persona_type\": persona[\"name\"]\n",
        "    }\n",
        "\n",
        "\n",
        "def generate_session_inputs(user_prefs, template):\n",
        "    \"\"\"Generate session inputs matching recommend() function parameters\"\"\"\n",
        "    base_budget_min = user_prefs.get(\"budget_min\", 2000)\n",
        "    base_budget_max = user_prefs.get(\"budget_max\", 10000)\n",
        "    multiplier = template[\"budget_multiplier\"]\n",
        "\n",
        "    # Session-specific preferences (may differ slightly from user profile)\n",
        "    session_styles = user_prefs.get(\"favorite_styles\", [\"classic\"])\n",
        "\n",
        "    # Sometimes add the occasion's style hint\n",
        "    if random.random() < 0.3 and template[\"style_hint\"] not in session_styles:\n",
        "        session_styles = session_styles + [template[\"style_hint\"]]\n",
        "\n",
        "    # Mix user colors with outfit colors\n",
        "    user_colors = user_prefs.get(\"preferred_colors\", [])\n",
        "    outfit_colors = random.sample(template[\"colors\"], k=min(2, len(template[\"colors\"])))\n",
        "    session_colors = list(set(user_colors[:1] + outfit_colors[:1]))  # Mix of both\n",
        "\n",
        "    return {\n",
        "        \"preferred_styles\": session_styles,\n",
        "        \"preferred_colors\": session_colors,\n",
        "        \"min_price\": int(base_budget_min * multiplier * random.uniform(0.8, 1.0)),\n",
        "        \"max_price\": int(base_budget_max * multiplier * random.uniform(0.9, 1.2)),\n",
        "        \"min_quantity\": 1\n",
        "    }\n",
        "\n",
        "\n",
        "def calculate_recommendation_scores(item, session_inputs, cluster_info):\n",
        "    \"\"\"\n",
        "    Calculate scores matching ML model's scoring system:\n",
        "    - Style Score (40%): max 4.0 points\n",
        "    - Color Score (30%): max 3.0 points\n",
        "    - Cluster Bonus (30%): max 3.0 points\n",
        "    - Total: max 10.0 points\n",
        "    \"\"\"\n",
        "    preferred_styles = session_inputs.get(\"preferred_styles\", [])\n",
        "    preferred_colors = session_inputs.get(\"preferred_colors\", [])\n",
        "\n",
        "    # Style score\n",
        "    style_score = 0.0\n",
        "    if cluster_info and cluster_info.get(\"primary_style\") in preferred_styles:\n",
        "        style_score = random.uniform(2.5, 4.0)  # Good match\n",
        "    elif cluster_info:\n",
        "        style_score = random.uniform(0.5, 2.0)  # Partial match\n",
        "\n",
        "    # Color score\n",
        "    color_score = 0.0\n",
        "    item_colors = item.get(\"colors\", [])\n",
        "    if item_colors and preferred_colors:\n",
        "        # Check for color family matches\n",
        "        matches = 0\n",
        "        for pref_color in preferred_colors:\n",
        "            family = COLOR_FAMILIES.get(pref_color, [pref_color])\n",
        "            if any(ic in family for ic in item_colors):\n",
        "                matches += 1\n",
        "        color_score = min(matches / len(preferred_colors), 1.0) * 3.0\n",
        "\n",
        "    # Cluster bonus\n",
        "    cluster_bonus = 0.0\n",
        "    if cluster_info:\n",
        "        cluster_style = cluster_info.get(\"primary_style\", \"\")\n",
        "        if cluster_style in preferred_styles:\n",
        "            cluster_bonus = random.uniform(1.5, 3.0)  # Strong alignment\n",
        "        else:\n",
        "            cluster_bonus = random.uniform(0.0, 1.0)  # Weak alignment\n",
        "\n",
        "    # Add some noise to make it realistic\n",
        "    style_score = max(0, min(4.0, style_score + random.uniform(-0.3, 0.3)))\n",
        "    color_score = max(0, min(3.0, color_score + random.uniform(-0.2, 0.2)))\n",
        "    cluster_bonus = max(0, min(3.0, cluster_bonus + random.uniform(-0.2, 0.2)))\n",
        "\n",
        "    total_score = style_score + color_score + cluster_bonus\n",
        "\n",
        "    return {\n",
        "        \"style_score\": round(style_score, 2),\n",
        "        \"color_score\": round(color_score, 2),\n",
        "        \"cluster_bonus\": round(cluster_bonus, 2),\n",
        "        \"total_score\": round(total_score, 2)\n",
        "    }\n",
        "\n",
        "\n",
        "# MAIN GENERATION FUNCTIONS\n",
        "\n",
        "def generate_users(conn, num_users):\n",
        "    \"\"\"Generate user profiles\"\"\"\n",
        "    print(f\"\\nGenerating {num_users} users...\")\n",
        "    users = []\n",
        "\n",
        "    for i in range(num_users):\n",
        "        persona = weighted_choice(USER_PERSONAS)\n",
        "        name = generate_name()\n",
        "        email = generate_email(name)\n",
        "        preferences = generate_user_preferences(persona)\n",
        "\n",
        "        try:\n",
        "            result = conn.execute(text(\"\"\"\n",
        "                INSERT INTO user_profiles (email, name, preferences)\n",
        "                VALUES (:email, :name, :prefs)\n",
        "                ON CONFLICT (email) DO UPDATE SET\n",
        "                    name = EXCLUDED.name,\n",
        "                    preferences = EXCLUDED.preferences\n",
        "                RETURNING id\n",
        "            \"\"\"), {\n",
        "                'email': email,\n",
        "                'name': name,\n",
        "                'prefs': json.dumps(preferences)\n",
        "            })\n",
        "            user_id = result.fetchone()[0]\n",
        "            users.append({\n",
        "                'id': user_id,\n",
        "                'email': email,\n",
        "                'preferences': preferences,\n",
        "                'persona': persona['name']\n",
        "            })\n",
        "\n",
        "        except Exception as e:\n",
        "            print(f\"  Error creating user {i}: {e}\")\n",
        "\n",
        "    conn.commit()\n",
        "    print(f\"✓ Created {len(users)} users\")\n",
        "\n",
        "    # Print persona distribution\n",
        "    persona_counts = {}\n",
        "    for u in users:\n",
        "        p = u['persona']\n",
        "        persona_counts[p] = persona_counts.get(p, 0) + 1\n",
        "    print(\"  Persona distribution:\")\n",
        "    for p, c in sorted(persona_counts.items(), key=lambda x: -x[1]):\n",
        "        print(f\"    {p}: {c}\")\n",
        "\n",
        "    return users\n",
        "\n",
        "\n",
        "def generate_sessions(conn, users, num_sessions):\n",
        "    \"\"\"Generate outfit sessions with ML-compatible inputs\"\"\"\n",
        "    print(f\"\\nGenerating {num_sessions} sessions...\")\n",
        "    sessions = []\n",
        "\n",
        "    # Generate sessions over past 6 months\n",
        "    end_date = datetime.now()\n",
        "    start_date = end_date - timedelta(days=180)\n",
        "\n",
        "    for i in range(num_sessions):\n",
        "        user = random.choice(users)\n",
        "        template = weighted_choice(OUTFIT_TEMPLATES)\n",
        "\n",
        "        session_inputs = generate_session_inputs(user['preferences'], template)\n",
        "\n",
        "        # Random timestamp\n",
        "        random_days = random.randint(0, 180)\n",
        "        session_time = start_date + timedelta(\n",
        "            days=random_days,\n",
        "            hours=random.randint(8, 22),\n",
        "            minutes=random.randint(0, 59)\n",
        "        )\n",
        "\n",
        "        clothing_features = {\n",
        "            \"occasion\": template[\"occasion\"],\n",
        "            \"outfit_colors\": random.sample(template[\"colors\"], k=min(2, len(template[\"colors\"]))),\n",
        "            \"style_hint\": template[\"style_hint\"],\n",
        "            \"season\": random.choice([\"spring\", \"summer\", \"fall\", \"winter\"])\n",
        "        }\n",
        "\n",
        "        try:\n",
        "            result = conn.execute(text(\"\"\"\n",
        "                INSERT INTO outfit_sessions\n",
        "                (user_id, preferred_colors, preferred_styles, min_price, max_price,\n",
        "                 min_quantity, clothing_features, created_at)\n",
        "                VALUES (:user_id, :colors, :styles, :min_price, :max_price,\n",
        "                        :min_qty, :clothing, :created_at)\n",
        "                RETURNING id\n",
        "            \"\"\"), {\n",
        "                'user_id': user['id'],\n",
        "                'colors': session_inputs['preferred_colors'],\n",
        "                'styles': session_inputs['preferred_styles'],\n",
        "                'min_price': session_inputs['min_price'],\n",
        "                'max_price': session_inputs['max_price'],\n",
        "                'min_qty': session_inputs['min_quantity'],\n",
        "                'clothing': json.dumps(clothing_features),\n",
        "                'created_at': session_time\n",
        "            })\n",
        "            session_id = result.fetchone()[0]\n",
        "            sessions.append({\n",
        "                'id': session_id,\n",
        "                'user': user,\n",
        "                'inputs': session_inputs,\n",
        "                'template': template,\n",
        "                'created_at': session_time\n",
        "            })\n",
        "\n",
        "        except Exception as e:\n",
        "            print(f\"  Error creating session {i}: {e}\")\n",
        "\n",
        "    conn.commit()\n",
        "    print(f\"✓ Created {len(sessions)} sessions\")\n",
        "    return sessions\n",
        "\n",
        "\n",
        "def generate_recommendations(conn, sessions, items_per_session=5):\n",
        "    \"\"\"Generate recommendation logs with realistic score breakdowns\"\"\"\n",
        "    print(f\"\\nGenerating recommendations ({items_per_session} per session)...\")\n",
        "\n",
        "    # Load items with cluster info\n",
        "    items_df = pd.read_sql(\"\"\"\n",
        "        SELECT\n",
        "            j.id, j.ref, j.price, j.collection, j.cluster_id,\n",
        "            c.name as category,\n",
        "            cl.primary_style as cluster_style,\n",
        "            cl.style_confidence,\n",
        "            ARRAY_AGG(DISTINCT m.color) FILTER (WHERE m.color IS NOT NULL) as colors\n",
        "        FROM jewelry_items j\n",
        "        JOIN categories c ON j.category_id = c.id\n",
        "        LEFT JOIN clusters cl ON j.cluster_id = cl.id\n",
        "        JOIN inventory i ON j.id = i.item_id\n",
        "        LEFT JOIN item_materials im ON j.id = im.item_id\n",
        "        LEFT JOIN materials m ON im.material_id = m.id\n",
        "        WHERE i.status = 'active' AND i.stock_qty > 0\n",
        "        GROUP BY j.id, j.ref, j.price, j.collection, j.cluster_id,\n",
        "                 c.name, cl.primary_style, cl.style_confidence\n",
        "    \"\"\", conn)\n",
        "\n",
        "    if len(items_df) == 0:\n",
        "        print(\"✗ No items found. Run import_to_supabase_v2.py first!\")\n",
        "        return\n",
        "\n",
        "    # Get cluster info lookup\n",
        "    clusters_df = pd.read_sql(\"SELECT id, primary_style, style_confidence FROM clusters\", conn)\n",
        "    cluster_lookup = {row['id']: row.to_dict() for _, row in clusters_df.iterrows()}\n",
        "\n",
        "    total_recs = 0\n",
        "    total_clicks = 0\n",
        "    total_purchases = 0\n",
        "\n",
        "    for session in sessions:\n",
        "        session_inputs = session['inputs']\n",
        "        min_price = session_inputs.get('min_price', 0)\n",
        "        max_price = session_inputs.get('max_price', 999999)\n",
        "        preferred_styles = session_inputs.get('preferred_styles', [])\n",
        "\n",
        "        # Filter items by price (with some flexibility)\n",
        "        filtered = items_df[\n",
        "            (items_df['price'] >= min_price * 0.7) &\n",
        "            (items_df['price'] <= max_price * 1.3)\n",
        "        ].copy()\n",
        "\n",
        "        if len(filtered) < items_per_session:\n",
        "            filtered = items_df.sample(min(items_per_session, len(items_df)))\n",
        "\n",
        "        # Score and rank items\n",
        "        scored_items = []\n",
        "        for _, item in filtered.iterrows():\n",
        "            cluster_info = cluster_lookup.get(item['cluster_id'], {})\n",
        "            scores = calculate_recommendation_scores(\n",
        "                item.to_dict(),\n",
        "                session_inputs,\n",
        "                cluster_info\n",
        "            )\n",
        "            scored_items.append({\n",
        "                'item': item,\n",
        "                'scores': scores,\n",
        "                'cluster_style': cluster_info.get('primary_style', 'unknown')\n",
        "            })\n",
        "\n",
        "        # Sort by total score and take top K\n",
        "        scored_items.sort(key=lambda x: x['scores']['total_score'], reverse=True)\n",
        "        top_items = scored_items[:items_per_session]\n",
        "\n",
        "        # Insert recommendations\n",
        "        for rank, rec in enumerate(top_items, 1):\n",
        "            item = rec['item']\n",
        "            scores = rec['scores']\n",
        "\n",
        "            # Determine click/purchase based on score\n",
        "            # Higher scores = more likely to click\n",
        "            click_prob = min(0.5, scores['total_score'] / 15)  # Max ~50% CTR for high scores\n",
        "            clicked = random.random() < click_prob\n",
        "\n",
        "            # 20% of clicks convert to purchase\n",
        "            purchased = clicked and random.random() < 0.20\n",
        "\n",
        "            click_time = None\n",
        "            purchase_time = None\n",
        "            if clicked:\n",
        "                click_time = session['created_at'] + timedelta(minutes=random.randint(1, 30))\n",
        "                if purchased:\n",
        "                    purchase_time = click_time + timedelta(minutes=random.randint(5, 120))\n",
        "\n",
        "            try:\n",
        "                conn.execute(text(\"\"\"\n",
        "                    INSERT INTO recommendation_log\n",
        "                    (session_id, item_id, rank, total_score, style_score, color_score,\n",
        "                     cluster_bonus, cluster_style, clicked, purchased,\n",
        "                     click_timestamp, purchase_timestamp, created_at)\n",
        "                    VALUES (:session_id, :item_id, :rank, :total_score, :style_score,\n",
        "                            :color_score, :cluster_bonus, :cluster_style, :clicked,\n",
        "                            :purchased, :click_ts, :purchase_ts, :created_at)\n",
        "                \"\"\"), {\n",
        "                    'session_id': session['id'],\n",
        "                    'item_id': int(item['id']),\n",
        "                    'rank': rank,\n",
        "                    'total_score': scores['total_score'],\n",
        "                    'style_score': scores['style_score'],\n",
        "                    'color_score': scores['color_score'],\n",
        "                    'cluster_bonus': scores['cluster_bonus'],\n",
        "                    'cluster_style': rec['cluster_style'],\n",
        "                    'clicked': clicked,\n",
        "                    'purchased': purchased,\n",
        "                    'click_ts': click_time,\n",
        "                    'purchase_ts': purchase_time,\n",
        "                    'created_at': session['created_at']\n",
        "                })\n",
        "\n",
        "                total_recs += 1\n",
        "                if clicked:\n",
        "                    total_clicks += 1\n",
        "                if purchased:\n",
        "                    total_purchases += 1\n",
        "\n",
        "            except Exception as e:\n",
        "                print(f\"  Error logging recommendation: {e}\")\n",
        "\n",
        "    conn.commit()\n",
        "\n",
        "    ctr = (total_clicks / total_recs * 100) if total_recs > 0 else 0\n",
        "    cvr = (total_purchases / total_clicks * 100) if total_clicks > 0 else 0\n",
        "\n",
        "    print(f\"✓ Created {total_recs} recommendations\")\n",
        "    print(f\"  - Clicks: {total_clicks} ({ctr:.1f}% CTR)\")\n",
        "    print(f\"  - Purchases: {total_purchases} ({cvr:.1f}% conversion)\")\n",
        "\n",
        "\n",
        "def print_final_summary(conn):\n",
        "    \"\"\"Print comprehensive summary\"\"\"\n",
        "    print(\"\\n\" + \"=\" * 60)\n",
        "    print(\"DATA GENERATION SUMMARY\")\n",
        "    print(\"=\" * 60)\n",
        "\n",
        "    # User stats\n",
        "    result = conn.execute(text(\"\"\"\n",
        "        SELECT\n",
        "            preferences->>'persona_type' as persona,\n",
        "            COUNT(*) as count\n",
        "        FROM user_profiles\n",
        "        GROUP BY preferences->>'persona_type'\n",
        "        ORDER BY count DESC\n",
        "    \"\"\"))\n",
        "    print(\"\\nUsers by Persona:\")\n",
        "    for row in result.fetchall():\n",
        "        print(f\"  {row[0]}: {row[1]}\")\n",
        "\n",
        "    # Session stats by occasion\n",
        "    result = conn.execute(text(\"\"\"\n",
        "        SELECT\n",
        "            clothing_features->>'occasion' as occasion,\n",
        "            COUNT(*) as count\n",
        "        FROM outfit_sessions\n",
        "        GROUP BY clothing_features->>'occasion'\n",
        "        ORDER BY count DESC\n",
        "    \"\"\"))\n",
        "    print(\"\\nSessions by Occasion:\")\n",
        "    for row in result.fetchall():\n",
        "        print(f\"  {row[0]}: {row[1]}\")\n",
        "\n",
        "    # Recommendation stats by cluster style\n",
        "    result = conn.execute(text(\"\"\"\n",
        "        SELECT\n",
        "            cluster_style,\n",
        "            COUNT(*) as recs,\n",
        "            SUM(CASE WHEN clicked THEN 1 ELSE 0 END) as clicks,\n",
        "            SUM(CASE WHEN purchased THEN 1 ELSE 0 END) as purchases,\n",
        "            ROUND(AVG(total_score), 2) as avg_score\n",
        "        FROM recommendation_log\n",
        "        WHERE cluster_style IS NOT NULL\n",
        "        GROUP BY cluster_style\n",
        "        ORDER BY recs DESC\n",
        "    \"\"\"))\n",
        "    print(\"\\nRecommendations by Cluster Style:\")\n",
        "    print(f\"  {'Style':<12} {'Recs':>6} {'Clicks':>7} {'Purchases':>10} {'Avg Score':>10}\")\n",
        "    print(f\"  {'-'*12} {'-'*6} {'-'*7} {'-'*10} {'-'*10}\")\n",
        "    for row in result.fetchall():\n",
        "        print(f\"  {row[0]:<12} {row[1]:>6} {row[2]:>7} {row[3]:>10} {row[4]:>10}\")\n",
        "\n",
        "    # Overall stats\n",
        "    result = conn.execute(text(\"\"\"\n",
        "        SELECT\n",
        "            COUNT(*) as total_recs,\n",
        "            SUM(CASE WHEN clicked THEN 1 ELSE 0 END) as clicks,\n",
        "            SUM(CASE WHEN purchased THEN 1 ELSE 0 END) as purchases,\n",
        "            ROUND(AVG(total_score), 2) as avg_score,\n",
        "            ROUND(AVG(style_score), 2) as avg_style,\n",
        "            ROUND(AVG(color_score), 2) as avg_color,\n",
        "            ROUND(AVG(cluster_bonus), 2) as avg_cluster\n",
        "        FROM recommendation_log\n",
        "    \"\"\"))\n",
        "    row = result.fetchone()\n",
        "    print(f\"\\nOverall Metrics:\")\n",
        "    print(f\"  Total Recommendations: {row[0]}\")\n",
        "    print(f\"  Click-Through Rate: {row[1]/row[0]*100:.1f}%\")\n",
        "    print(f\"  Conversion Rate: {row[2]/row[1]*100:.1f}%\" if row[1] > 0 else \"  Conversion Rate: N/A\")\n",
        "    print(f\"\\nAverage Scores:\")\n",
        "    print(f\"  Total: {row[3]} | Style: {row[4]} | Color: {row[5]} | Cluster: {row[6]}\")\n",
        "\n",
        "    print(\"=\" * 60)\n",
        "\n",
        "\n",
        "# RUN\n",
        "\n",
        "def main():\n",
        "    print(\"=\" * 60)\n",
        "    print(\"SYNTHETIC DATA GENERATOR (V2)\")\n",
        "    print(\"Aligned with K-Modes ML Model\")\n",
        "    print(\"=\" * 60)\n",
        "\n",
        "    # Connect\n",
        "    print(\"\\nConnecting to database...\")\n",
        "    try:\n",
        "        engine = create_engine(DATABASE_URL)\n",
        "        with engine.connect() as conn:\n",
        "            conn.execute(text(\"SELECT 1\"))\n",
        "        print(\"✓ Connected!\")\n",
        "    except Exception as e:\n",
        "        print(f\"✗ Connection failed: {e}\")\n",
        "        print(\"\\nPlease check:\")\n",
        "        print(\"  - DATABASE_URL is correct\")\n",
        "        print(\"  - Supabase project is active\")\n",
        "        return\n",
        "\n",
        "    # Check if items exist\n",
        "    with engine.connect() as conn:\n",
        "        result = conn.execute(text(\"SELECT COUNT(*) FROM jewelry_items\"))\n",
        "        item_count = result.fetchone()[0]\n",
        "        if item_count == 0:\n",
        "            print(\"\\n✗ No jewelry items found!\")\n",
        "            print(\"  Please run import_to_supabase_v2.py first.\")\n",
        "            return\n",
        "        print(f\"✓ Found {item_count} jewelry items\")\n",
        "\n",
        "    # Generate data\n",
        "    with engine.connect() as conn:\n",
        "        users = generate_users(conn, NUM_USERS)\n",
        "        sessions = generate_sessions(conn, users, NUM_SESSIONS)\n",
        "        generate_recommendations(conn, sessions, NUM_RECOMMENDATIONS_PER_SESSION)\n",
        "        print_final_summary(conn)\n",
        "\n",
        "    print(\"\\n✓ Data generation complete!\")\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ]
    }
  ]
}